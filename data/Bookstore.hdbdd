: String(1) default 'N';    };    entity CategoryKey {        key Category : CategoryT;    };    entity CategoryText {        key Category : CategoryT;        key Language : String(2);        Description  : String(250);    };        context CustomerOrder {                entity Header {            key ID        : BusinessKey;                Customer  : UserNameT;                Currency  : CurrencyKey;                RequestedDeliveryDate : LocalDate;        };    };}; 